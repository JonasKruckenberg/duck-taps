{
  "name": "duck-taps",
  "version": "0.0.1",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "files"
  ],
  "author": "Jonas Kruckenberg <jonas@kruckenbg.de>",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -cw",
    "test": "mocha",
    "test-live": "mocha --watch --watch-files src --reporter min",
    "coverage": "nyc -r text yarn test",
    "docs": "typedoc --options ./typedoc.json",
    "commit": "git cz"
  },
  "release": {
    "branches": [
      {
        "name": "master"
      },
      {
        "name": "next",
        "channel": "next",
        "prerelease": true
      }
    ]
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "chai": "^4.2.0",
    "cz-conventional-changelog": "3.1.0",
    "mocha": "^7.1.1",
    "nyc": "^15.0.1",
    "rollup": "^2.6.1",
    "rollup-plugin-typescript2": "^0.27.0",
    "semantic-release": "^17.0.4",
    "ts-node": "^8.8.2",
    "typedoc": "^0.17.4",
    "typedoc-plugin-markdown": "^2.2.17",
    "typescript": "^3.8.3"
  },
  "dependencies": {},
  "keywords": [
    "hooks",
    "taps",
    "tapable",
    "plugins"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
